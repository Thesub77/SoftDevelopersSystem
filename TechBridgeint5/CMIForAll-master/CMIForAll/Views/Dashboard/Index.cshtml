
@{
    ViewBag.Title = "Index";
}
<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Tech Bridge</title>
    <link rel="shortcut icon" type="image/png" href="~/img/tech_icon.png" />
    <link rel="stylesheet" href="~/Style/Style.css" />
    <script src="https://cdn.jsdelivr.net/npm/echarts/dist/echarts.min.js"></script>

</head>

<body>
    <br>
    <br>
    <br>
    <br>
    <br>
    <!--  Header End -->
    <div class="container-fluid">
        <!--  Grafico princiapal -->
        <div class="row">
            <div class="col-lg-8 d-flex align-items-stretch">
                <div class="card w-100">
                    <div class="card-body p-4">
                        <h5 class="card-title mb-9 fw-semibold text-center">Proyectos entregados por año</h5>
                        <div id="graficoProyectosPorAnio" style="width: 100%; height: 360px;"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="row">
                    <div class="col-lg-12">
                        <!-- Yearly Breakup -->
                        <div class="card overflow-hidden">
                            <div class="card-body p-4">
                                <h5 class="card-title mb-9 fw-semibold">Ingresos por mes</h5>
                                <div class="row align-items-center">
                                    <div class="col-24">
                                        <div id="graficoGananciaPorMes" style="width: 100%; height: 250px;"></div>

                                    </div>

                                </div>
                            </div>
                        </div>

                        <div class="col-lg-12">
                            <!-- Monthly Earnings -->
                            <div class="card">
                                <div class="card-body">
                                    <div class="row align-items-start">
                                        <div class="col-8">
                                            <h5 class="card-title mb-9 fw-semibold">Proyectos en ejecución</h5>
                                            <h4 class="fw-semibold mb-3">100</h4>
                                            <div class="d-flex align-items-center pb-1">
                                                <span class="me-2 rounded-circle bg-light-danger round-20 d-flex align-items-center justify-content-center">
                                                    <i class="ti ti-arrow-down-right text-danger"></i>
                                                </span>
                                                <p class="text-dark me-1 fs-3 mb-0">+20</p>
                                                <p class="fs-3 mb-0">Del mes pasado</p>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="d-flex justify-content-end">
                                                <div class="text-white bg-secondary rounded-circle p-6 d-flex align-items-center justify-content-center">
                                                    <i class="ti ti-currency-dollar fs-6"></i>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div id="earning"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-8 d-flex align-items-stretch">
            <div class="card w-100">
                <div class="card-body p-4">
                    <h5 class="card-title fw-semibold mb-4">Ultimos Proyectos</h5>
                    <div class="table-responsive">
                        <table class="table text-nowrap mb-0 align-middle">
                            <thead class="text-dark fs-4">
                                <tr>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Id</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Gerente de proyecto</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Nombe del proyecto</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Empresa</h6>
                                    </th>
                                    <th class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0">Monto</h6>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="border-bottom-0"><h6 class="fw-semibold mb-0">1</h6></td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-semibold mb-1">Maria Vanegas</h6>
                                        <span class="fw-normal">Web Designer</span>
                                    </td>
                                    <td class="border-bottom-0">
                                        <p class="mb-0 fw-normal">CodeCrafters</p>
                                    </td>
                                    <td class="border-bottom-0">
                                        <p class="mb-0 fw-normal">Empremar S.A</p>
                                    </td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0 fs-4">$20,000</h6>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="border-bottom-0"><h6 class="fw-semibold mb-0">2</h6></td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-semibold mb-1">Dexter Urbina</h6>
                                        <span class="fw-normal">Web Designer</span>
                                    </td>
                                    <td class="border-bottom-0">
                                        <p class="mb-0 fw-normal">BinaryBuilders</p>
                                    </td>
                                    <td class="border-bottom-0">
                                        <p class="mb-0 fw-normal">Parmalat</p>
                                    </td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0 fs-4">$10,000</h6>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="border-bottom-0"><h6 class="fw-semibold mb-0">3</h6></td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-semibold mb-1">Maria Vanegas</h6>
                                        <span class="fw-normal">Web Designer</span>
                                    </td>
                                    <td class="border-bottom-0">
                                        <p class="mb-0 fw-normal">TechTrendsetters</p>
                                    </td>
                                    <td class="border-bottom-0">
                                        <p class="mb-0 fw-normal">Sitel S.A</p>
                                    </td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0 fs-4">$30,500</h6>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="border-bottom-0"><h6 class="fw-semibold mb-0">4</h6></td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-semibold mb-1">Santiagio Lopez</h6>
                                        <span class="fw-normal">Web Designer</span>
                                    </td>
                                    <td class="border-bottom-0">
                                        <p class="mb-0 fw-normal">DigitalDreamworks</p>
                                    </td>
                                    <td class="border-bottom-0">
                                        <p class="mb-0 fw-normal">Ferrari</p>
                                    </td>
                                    <td class="border-bottom-0">
                                        <h6 class="fw-semibold mb-0 fs-4">$100,500</h6>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>


        <!-- Your existing scripts -->

        <script src="~/Java/GraficosDashApi.js"></script>
        <script src="~/Java/app.min.js"></script>
        <script src="~/Java/jquery.min.js"></script>
        <script src="~/Java/simplebar.js"></script>
        <script src="~/Java/bootstrap.bundle.min.js"></script>
        <script src="~/Java/apexxcharts.js"></script>
        <script src="~/Java/dashboard.js"></script>
       

        @section Scripts{
            <script>
                $(document).ready(() => {
                    Cantidadporanio();
                })
            </script>
            <script>
                $(document).ready(() => {
                    margenGananciasTrimestre();
                })
            </script>
        }

        <!-- Initialize the chart -->

</body>

</html>
